<bundle version="0.10.4">
  <workflow name="GenomicAlignmentNovoalign" version="0.10.4">
    <description>An alignment workflow using Novoalign to hg19 references prepared by OICR.</description>
    <test command="java -jar ${workflow_bundle_dir}/Workflow_Bundle_GenomicAlignmentNovoalign/0.10.4/lib/seqware-distribution-0.13.5-full.jar --plugin net.sourceforge.seqware.pipeline.plugins.WorkflowLauncher -- --no-metadata --bundle ${workflow_bundle_dir} --workflow GenomicAlignmentNovoalign --version 0.10.4 --ini-files ${workflow_bundle_dir}/Workflow_Bundle_GenomicAlignmentNovoalign/0.10.4/config/GenomicAlignment/GenomicAlignmentNovoalign_0.10.4.ini "/>
    <workflow_command command="java -jar ${workflow_bundle_dir}/Workflow_Bundle_GenomicAlignmentNovoalign/0.10.4/lib/seqware-distribution-0.13.5-full.jar --plugin net.sourceforge.seqware.pipeline.plugins.WorkflowLauncher -- --provisioned-bundle-dir ${workflow_bundle_dir} --workflow GenomicAlignmentNovoalign --version 0.10.4 "/>
    <workflow_template path="${workflow_bundle_dir}/Workflow_Bundle_GenomicAlignmentNovoalign/0.10.4/workflows/GenomicAlignment/GenomicAlignmentNovoalign_0.10.4.ftl"/>
    <config path="${workflow_bundle_dir}/Workflow_Bundle_GenomicAlignmentNovoalign/0.10.4/config/GenomicAlignment/GenomicAlignmentNovoalign_0.10.4.ini"/>
    <build command="ant -f ${workflow_bundle_dir}/Workflow_Bundle_GenomicAlignmentNovoalign/0.10.4/build.xml"/>
    <requirements compute="cluster" memory="16G" network="local"/>
  </workflow>
</bundle>
